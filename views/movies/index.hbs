<h2>Movies</h2>
<ul>
{{#each allMovies}}
  <li>
      <a href="/movies/{{id}}">{{title}}</a>
      <form action="/movies/{{id}}/delete" method="POST">
        <a href="/movies/{{id}}/edit">Edit</a>
        <button type="submit">Delete</button>
      </form>
  </li>
{{/each}}
</ul>
<a href="/"><button>Back</button></a>
<a href="/movies/new"><button>Add movie</button></a>